<h2 id="code"></h2>
<button id="button">generate code</button>
<a id="my" href>proceed</a>
<script src="/uuid/uuidv4.js"></script>
<script>
	var id = document.getElementById("code");
	var button = document.getElementById("button");
	
	function uuidv4() {
		return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
			var r = Math.random() * 16 | 0, v = c == 'x' ? r : (r & 0x3 | 0x8);
		    return v.toString(16);
	  });
	}
	
	//id.textcontent = uuidv4();
	button.addEventListener("click",function(){
		
		id.innerHTML = 	uuidv4();
		//document.getElementById("text").value= id.textContent;
		document.getElementById("my").href= "/snake?name="+id.textContent+"hoast";
		// alert(id.textcontent);
		
	})
	

console.log(uuidv4());


</script>